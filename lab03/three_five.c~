#include<stdio.h>


int main(void){
    int number;
    int count=1;
    printf("Enter number: ");
    scanf("%d",&number);
    while(count<number){
        if((count%3 == 0) || (count%5 == 0)){
        printf("%d\n",count);
        }      
      count=count+1;
        }
        return 0;
        }
