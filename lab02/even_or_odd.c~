#include<stdio.h>


int main(void){
    int number;
    printf("Please enter a number: ");
    scanf("%d",&number);
    if (number%2 == 0 & number > 0){
    printf("EVEN\n");
    }else if(number%2 == 1 & number > 0){
    printf("ODD\n");
    }
    else if(number < 0){
    printf("NEGATIVE\n");
    }
    return 0;
    }
